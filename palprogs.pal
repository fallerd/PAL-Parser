SRT ;if >
MOVE 0, R0
MOVE 0, R1
BGT R0, R1, IF
INC R0
BR END
IF: DEC R0
END: END
;whatevertest
SRT ;for
MOVE 0, R0
MOVE 0, R1
MOVE 11, R2  ;9 in octal
FOR: BGT R1, R2, END
ADD R0, R1, R0
INC R1
BR FOR
END: END

SRT: ;while
MOVE 0, R0
MOVE 0, R1
MOVE 11, R2  ;9 in octal
WHILE: BGT R1, R2, END
ADD R0, R1, R0
INC R1
BR WHILE
END: END

SRT: ;if==
MOVE 12, R0 ;10 in octal
MOVE 12, R1
BEQ R0, R1, EQ
MOVE 12, R0
BR END
EQ: MOVE 24, R0
END: END
